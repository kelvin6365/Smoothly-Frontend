	"scripts": {
		"start": "webpack-dev-server --host 0.0.0.0",
		"build": "webpack --mode production",
		"test": "jest",
		"eject": "react-scripts eject",
		"relay": "relay-compiler --src ./src --schema ./schema.graphql"
	},
	"jest": {
		"snapshotSerializers": [
